fonts = DINNextLTPro-Black

for font in fonts   
	@font-face
		font-family: font
		src: url($assetspath + '/fonts/'font'.woff') format('woff'), url($assetspath + '/fonts/'font'.woff2') format('woff2')
		font-weight: normal
		font-style: normal

regular()
	font-family 'DINNextLTPro-Black'
	font-weight normal
body
	regular()
h1,h2,h3,h4,h5
	regular()
p + p
	margin-top 1.6rem
